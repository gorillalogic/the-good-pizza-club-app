{
  "name": "the-good-pizza-club-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@fontsource/rowdies": "^4.5.8",
    "@googlemaps/js-api-loader": "^1.15.1",
    "@mui/material": "^5.11.10",
    "@reduxjs/toolkit": "^1.9.3",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.12",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "axios": "^1.3.4",
    "date-fns": "^2.29.3",
    "embla-carousel-react": "^7.0.9",
    "react": "^18.2.0",
    "react-cookie": "^4.1.1",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.2",
    "react-payment-inputs": "^1.1.9",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.8.1",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --watchAll=false",
    "test:watch": "react-scripts test",
    "test:coverage": "react-scripts test --watchAll=false --coverage",
    "test:coverage:watch": "react-scripts test --watchAll --coverage",
    "eject": "react-scripts eject",
    "lint": "eslint .",
    "prepare": "husky install"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/",
      "!<rootDir>/src/**/*.d.ts",
      "!<rootDir>/src/core/lib/*",
      "!<rootDir>/src/models/*",
      "!<rootDir>/src/mocks/*",
      "!<rootDir>/src/index.tsx"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    },
    "transformIgnorePatterns": [
      "node_modules/(?!axios)"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@types/google.maps": "^3.52.4",
    "@types/react-payment-inputs": "^1.1.0",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "eslint": "^8.34.0",
    "eslint-plugin-react": "^7.32.2",
    "husky": "^8.0.3",
    "msw": "^1.2.1",
    "prettier-plugin-organize-imports": "^3.2.2",
    "sass": "^1.58.3"
  }
}
